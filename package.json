{
  "name": "yaad",
  "version": "1.0.0",
  "description": "Self-hosted personal media tracking application",
  "type": "module",
  "scripts": {
    "build:css": "tailwindcss -i ./static/css/input.css -o ./static/css/output.css --minify",
    "watch:css": "tailwindcss -i ./static/css/input.css -o ./static/css/output.css --watch",
    "dev": "npm run watch:css"
  },
  "keywords": ["media", "tracking", "self-hosted"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18"
  }
}
